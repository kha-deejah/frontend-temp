{
  "name": "html-css-template",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "hint": "npx hint --telemetry=off .",
    "format": "npx prettier  --write **/*.{html,js,json,md,json,yaml,yml,jsx}",
    "format:check": "npx prettier  --check **/*.{html,js,json,md,json,yaml,yml}",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:check": "npx eslint .",
    "stylelint": "npx stylelint **/*.css --fix",
    "stylelint:check": "npx stylelint **/*.css "
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "hint": "^7.1.13",
    "prettier": "3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1"
  }
}
